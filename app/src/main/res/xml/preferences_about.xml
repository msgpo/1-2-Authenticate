<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <Preference
        android:key="version"
        android:title="@string/version_preference_title"
        android:persistent="false"
        android:enabled="false"
        android:shouldDisableView="false" />
    <Preference
        android:key="author"
        android:title="@string/author"
        android:persistent="false"
        android:enabled="false"
        android:shouldDisableView="false"
        android:summary="wilco375">
        <intent
            android:action="android.intent.action.VIEW"
            android:data="@string/author_page_url" />
    </Preference>
    <PreferenceScreen
        android:key="opensource"
        android:title="@string/opensource_preference_title">
        <intent
            android:action="android.intent.action.VIEW"
            android:data="@string/opensource_page_url" />
    </PreferenceScreen>
</PreferenceScreen>
